FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    inletBoundary
    {
        type            zeroGradient;
    }
    outletBoundary
    {
        type            fixedValue;
        value           uniform 101325;
    }
    frontAndBack
    {
        type            empty;
    }
}


